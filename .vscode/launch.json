  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch openECOE API",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": { 
        "FLASK_APP": "openECOE-API.py", 
        "FLASK_ENV": "development",
        "FLASK_DEBUG": "0",
        "SERVER_NAME": "localhost",
        "DEBUG": "True",
        "TESTING": "False",
        "BCRYPT_LOG_ROUNDS": "4",
        "SQLALCHEMY_TRACK_MODIFICATIONS": "False",
        "API_AUTH": "True",
        "SQLALCHEMY_DATABASE_URI": "mysql+pymysql://openecoe:openecoe@localhost:8083/openECOE_dev",
        "RQ_REDIS_URL": "redis://localhost:6379/0",
        "CHRONO_ROUTE": "http://localhost:8082",
        "LOG_TO_STDOUT": "True"
      },
      "args": ["run"],
      "jinja": false,
      "justMyCode": true,
      "autoReload": {"enable": false}
    },
    {
      "name": "Launch RQ Worker",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": { 
        "FLASK_APP": "openECOE-API.py", 
        "FLASK_ENV": "development",
        "FLASK_DEBUG": "0",
      },
      "args": ["rq", "worker"],
      "jinja": false,
      "justMyCode": true,
      "envFile": "${workspaceFolder}/environments/dev.env",
      "autoReload": {"enable": false}
    },
    {"name": "Migrate DB",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": { 
        "FLASK_APP": "openECOE-API.py", 
        "FLASK_ENV": "development",
        "FLASK_DEBUG": "0",
      },
      "args": ["db", "migrate"],
      "jinja": false,
      "justMyCode": true,
      "envFile": "${workspaceFolder}/environments/dev.env",
      "autoReload": {"enable": false}
    },
    {"name": "Upgrade DB",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": { 
        "FLASK_APP": "openECOE-API.py", 
        "FLASK_ENV": "development",
        "FLASK_DEBUG": "0",
      },
      "args": ["db", "upgrade"],
      "jinja": false,
      "justMyCode": true,
      "envFile": "${workspaceFolder}/environments/dev.env",
      "autoReload": {"enable": false}
    },
    {"name": "Downgrade DB",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": { 
        "FLASK_APP": "openECOE-API.py", 
        "FLASK_ENV": "development",
        "FLASK_DEBUG": "0",
      },
      "args": ["db", "downgrade"],
      "jinja": false,
      "justMyCode": true,
      "envFile": "${workspaceFolder}/environments/dev.env",
      "autoReload": {"enable": false}
    },
    {"name": "Current revision DB",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": { 
        "FLASK_APP": "openECOE-API.py", 
        "FLASK_ENV": "development",
        "FLASK_DEBUG": "0",
      },
      "args": ["db", "current"],
      "jinja": false,
      "justMyCode": true,
      "envFile": "${workspaceFolder}/environments/dev.env",
      "autoReload": {"enable": false}
    }
  ]
}
